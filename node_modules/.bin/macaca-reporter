#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_macaca-reporter@1.3.31@macaca-reporter/bin/macaca-reporter.js" "$@"
  ret=$?
else 
  node  "$basedir/../_macaca-reporter@1.3.31@macaca-reporter/bin/macaca-reporter.js" "$@"
  ret=$?
fi
exit $ret
