#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_macaca-coverage@1.0.7@macaca-coverage/bin/macaca-coverage.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_macaca-coverage@1.0.7@macaca-coverage/bin/macaca-coverage.js" "$@"
  ret=$?
fi
exit $ret
