#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_remap-istanbul@0.11.1@remap-istanbul/bin/remap-istanbul.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_remap-istanbul@0.11.1@remap-istanbul/bin/remap-istanbul.js" "$@"
  ret=$?
fi
exit $ret
