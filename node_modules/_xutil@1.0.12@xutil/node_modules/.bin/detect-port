#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_detect-port@1.3.0@detect-port/bin/detect-port" "$@"
  ret=$?
else 
  node  "$basedir/../../../_detect-port@1.3.0@detect-port/bin/detect-port" "$@"
  ret=$?
fi
exit $ret
